{
  "productName": "Camera Example App",
  "version": "0.1.0",
  "identifier": "com.example.camera",
  "build": {
    "beforeDevCommand": "npm run dev -- --host 192.168.1.4",
    "beforeBuildCommand": "npm run build",
    "devUrl": "http://192.168.1.4:1420",
    "frontendDist": "../dist"
  },
  "app": {
    "withGlobalTauri": false,
    "security": {
      "csp": null
    },
    "windows": [
      {
        "fullscreen": false,
        "height": 600,
        "resizable": true,
        "title": "Camera Example App",
        "width": 800
      }
    ]
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico",
      "icons/icon.png"
    ]
  }
}